<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="tk.Toplevel" id="main">
    <property name="height">200</property>
    <property name="width">200</property>
    <property name="resizable">both</property>
    <child>
      <object class="ttk.Panedwindow" id="panel1">
        <property name="height">5</property>
        <property name="orient">vertical</property>
        <property name="takefocus">false</property>
        <property name="width">250</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Text" id="welcome">
        <property name="autoseparators">false</property>
        <property name="blockcursor">false</property>
        <property name="cursor">arrow</property>
        <property name="height">3</property>
        <property name="setgrid">false</property>
        <property name="takefocus">true</property>
        <property name="text" translatable="yes">Welcome to TranslateGame!
Click above to select a game.</property>
        <property name="width">29</property>
        <property name="wrap">char</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="game1">
        <property name="compound">center</property>
        <property name="state">normal</property>
        <property name="style">Toolbutton</property>
        <property name="takefocus">false</property>
        <property name="text" translatable="yes">Game 1</property>
        <property name="textvariable">string:game1</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">50</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="game2">
        <property name="compound">center</property>
        <property name="style">Toolbutton</property>
        <property name="takefocus">false</property>
        <property name="text" translatable="yes">Game 2</property>
        <property name="textvariable">string:game2</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">50</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
